{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "watchPatterns": [
      "/memorymap/**"
    ],
    "buildCommand": "cd memorymap && pip install -r requirements.txt && python -m spacy download xx_ent_wiki_sm"
  },
  "deploy": {
    "startCommand": "cd memorymap/backend && uvicorn main:app --host 0.0.0.0 --port $PORT",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3,
    "healthcheckPath": "/",
    "healthcheckTimeout": 100
  }
} 
